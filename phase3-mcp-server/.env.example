# Phase 3 MCP Server Configuration (.env.example)
# Copy this to .env and fill in your values

# Phase 2 Backend URL
# URL of the Phase 2 Backend API
PHASE2_BACKEND_URL=http://localhost:8000

# JWT Secret (MUST match Phase 2 backend and Phase 3 backend/frontend)
# Generate with: openssl rand -base64 32
JWT_SECRET=your-secret-key-here

# Server Configuration
PORT=8002
ENVIRONMENT=development

# Logging
LOG_LEVEL=INFO
